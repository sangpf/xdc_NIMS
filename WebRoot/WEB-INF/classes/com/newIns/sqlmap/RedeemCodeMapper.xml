<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.newIns.dao.redeemCode.RedeemCodeMapper">
	
	<!-- 新增记录 -->
	<insert id="insert" parameterType="com.newIns.model.redeemCode.RedeemCode">
		INSERT INTO ni_redeem_code 
		(redeemCode,provider,awardPoolId,userId,grantStatus,grantTime,phoneNum,comment)
		VALUES (#{redeemCode},#{provider},#{awardPoolId},#{userId},#{grantStatus},#{grantTime},#{phoneNum},#{comment})
	</insert>


</mapper>